@model IEnumerable<AppDevGroupCoursework.DTO.MemberDetailsNotTakenLoanDTO>
@{
    ViewData["Title"] = "MemberDetailsWhoHaveNotTakenLoanFor31days";
}

<h1>Member Details Who Have Not Taken Loan For 31days</h1>
                    <div class="card shadow mb-4">
<div class="card-body">
<div class="table-responsive">
<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
    <thead class="thead-dark">
        <tr>
            <th>
                First Name
            </th>
            <th>
                Last Name
            </th>
            <th>
                Address
            </th>
            <th>
                Title
            </th>
            <th>
                Last Date of Loan
            </th>
            <th>
                Days since last Loan
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.firstName
                </td>
                <td>
                    @item.lastName
                </td>
                <td>
                    @item.address
                </td>
                <td>
                    @item.DvdTitle
                </td>
                <td>
                    @item.dateOut
                </td>
                <td>
                    @item.numberOfDays
                </td>
            </tr>          
        }
    </tbody>
</table>
</div>
</div>
</div>



